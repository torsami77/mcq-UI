const sec = () => {
  let duration = 60;
  return setInterval(() => {
    duration -= 1;
    document.getElementById("sec").innerHTML = duration
  }, 1000);
 }
const timer = () => {
  let duration = 30;
  return setInterval(() => {
    duration -= 1;

    document.getElementById("demo").innerHTML = duration
  }, 1000 * 60);
 }

const starter = () => {
sec();
timer();
}
